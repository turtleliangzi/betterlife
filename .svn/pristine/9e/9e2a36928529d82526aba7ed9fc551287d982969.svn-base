/*
 * 用户登陆
 */
#login-box {
    width:1000px;
    height:100%;
    margin:0px auto;
    position:relative;
}   
#login-box .login-box-left {
    width:200px;
    height:200px;
    position:absolute;
    top:300px;
    left:200px;
}
#login-box .login-box-left p {
    font-size:20px;
    color:#fff;
    font-weight:bold;

}
#login-box .login-box-right::before{
    border-color:transparent #eceff1 transparent transparent;
    border-style:solid;
    border-width:12px 12px 12px 0;
    content:"";
    height:0;
    left:-12px;
    position:absolute;
    top:150px;
    width:0;
}
#login-box .login-box-right {
    background-color:#eceff1;
    border-radius:6px;
    padding:24px 23px 20px;
    position:absolute;
    width:400px;
    height:300px;
    top:150px;
    left:400px;

}
#login-box .login-box-right .control-group {
    margin-bottom:20px;
    position:relative;
    width:350px;
}
#login-box .login-box-right .control-group .login-field {
    border-colorr:transparent;
    font-size:17px;
    padding-bottom:11px;
    padding-top:11px;
    text-indent:8px;
    width:350px;
    border-radius:6px;
    border:2px solid #fff;
    color:#34495e;
    position:relative;

}
#login-box .login-box-right .control-group .login-btn {
    border:0 none;
    border-radius:6px;
    width:350px;
    background-color:#1abc9c;
    font-size:17px;
    font-weight:bold;
    height:45px;
    color:#fff;
    line-height:45px;
}
#login-box .login-box-right .control-group .login-btn:hover{
    background-color:#669966;
}
#login-box .login-box-right .control-group .login-icon {
    position:absolute;
    top:12px;
    color:#777;
    right:14px;
    font-size:24px;    
}
.message {
