"CHARSET UTF-8";
* {
    margin:0;
    padding:0;    
}
ul li {
    list-style:none;
}
body {
    background-color:#EEEEEE ;
    
}
a {
    text-decoration:none;
}
.form-control {
    box-shadow:none;
}
.form-control:focus {
    box-shadow:none;
}
.form-control[readonly] {
    background-color:#fff;
}
/*
 * BetterLife头部页面样式
 */
.bl-header {
    width:100%;
    height:60px;
    overflow:hidden;
    background-color:#669966;
    position:fixed;
    top:0px;
    left:0px;
    right:0px;
    z-index:100;
}
.bl-header .bl-header-bar {
    width:1060px;
    height:100%;
    margin:0 auto;
}
.bl-header .bl-header-bar .bl-logo {
    width:200px;
    height:60px;
    line-height:60px;
    position:relative;
    float:left;
    color:#fff;
    font-size:30px;
    font-weight:bold;
}
.bl-header .bl-header-bar .bl-header-bar-right {
    width:auto;
    height:60px;
    float:left;
}
.bl-header .bl-header-bar .bl-header-bar-right ul {
    width:100%;
    height:100%;
}
.bl-header .bl-header-bar .bl-header-bar-right ul li {
    display:inline-block;
    float:none;
    width:100px;
    height:60px;
    line-height:70px;
    text-align:center;
    color:#999;
    font-size:16px;
}
.bl-header .bl-header-bar .bl-header-bar-right ul li a{
    color: #fff;
}
.bl-header .bl-header-bar .bl-header-bar-right ul li a:hover{
    text-decoration:none;
}
/*
 * 首页
 */
#bl-content {
    width:1060px;
    height:auto;
    margin:0 auto;
    margin-top:80px;
}
#bl-content .bl-content-left {
    width:700px;
    height:auto;
    float:left;
}
#bl-content .bl-content-right {
    width:250px;
    height:auto;
    float:right;
}
.panel {
    border:0px;
    border-radius:0px;
    border-bottom-left-radius:3px;
    border-bottom-right-radius:3px;
}
.panel-footer {
    border-top:0px;
}
#bl-content .bl-content-left .bl-user-img {
    width:60px;
    height:60px;
    float:left;
}
#bl-content .bl-content-left .bl-user-img img {
    width:60px;
    height:60px;
}
#bl-content .bl-content-left .bl-user-nickname {
    height:60px;
    line-height:60px;
    font-size:18px;
    margin-left:70px;
    color:#999;
}
.panel-heading {
    padding-bottom:0px;
}
.panel-body {
    padding-top:5px;
}
#bl-content .bl-content-left .bl-articl-title {
    width:100%;
    height:25px;
    line-height:25px;

}
#bl-content .bl-content-left .bl-articl-content img {
    width:100%;
}
#bl-content .bl-content-left .bl-sort-tags {
    width:100%;
    height:25p;
    line-height:25px;
    color:#aaa;
    font-size:12px;
}
#bl-content .bl-content-left .bl-sort-tags span {
    margin-left:5px;
}
#bl-content .bl-content-left .panel-footer span {
    margin-right:10px;
}
#bl-content .bl-content-right .pin-wrapper .pinned {
    width:262px;
    margin-top:20px;
    position:relative;
}
#bl-content .bl-content-right .pin-wrapper .pinned .note {
    background:#ffc none repeat scroll 0 0;
    box-shadow:5px 5px 7px rgba(33, 33, 33, 0.7);
    display:block;
    font-size:18px;
    padding:20px;
    text-decoration:none;
    transform:rotate(6deg);
    width:230px;
}
#bl-content .bl-content-right .pin-wrapper .pinned .note h3 {
    font-size:32px;
    margin:0;    
}

