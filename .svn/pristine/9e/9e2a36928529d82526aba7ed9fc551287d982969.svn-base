.rating {
    unicode-bidi: bidi-override;
    direction: rtl;
    margin-left:20px;
    float:left;
}
.rating > span {
    display: inline-block;
    position: relative;
    width: 1.3em;
    font-size:20px;
}
.rating > span:hover:before,
.rating > span:hover ~ span:before {
    content: "\2605";
    position: absolute;
    color:#CC9900;

}
