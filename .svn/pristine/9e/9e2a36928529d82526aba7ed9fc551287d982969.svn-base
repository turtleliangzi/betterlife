.arrow.left::before {
    border-left-width:0px;
    border-right-color:rgha(0,0,0,0.1);
}   
.arrow::before , .arrow:after {
    border-color:transparent;
    border-style:solid;
    border-width:9px;
    content:" ";
    display:block;
    height:0;
    position:absolute;
    width:0px;
}
.arrow.left::after {
    border-bottom-color:transparent;
    border-left-color:transparent;
    border-left-width:0;
    border-right-color:inherit;
    border-top-color:transparent;
    left:1px;
    top:1px;
}
.arrow::after {
    border-width:8px;
}
.arrow.pull-top {
    top:18px;
}
.arrow.left {
    left:-9px;
    margin-top:-9px;
    top:50%;
}
.b-white {
    border-color:#fff;
}
.arrow {
    position:absolute;
    z-index:1;
}
